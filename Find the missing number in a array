import java.util.Scanner;

public class practice {
    public static void main(String[] args) {
       int[] arr = {1,2,3,0,5};
       int xor = 0;

       for(int i = 0 ; i<=arr.length ; i++){
        xor^=i;
       }
       for(int num : arr){
        xor^=num;
       }
       System.out.println(xor);
       
       
    }
}
